{
  "name": "lookescolar",
  "version": "0.1.0",
  "private": true,
  "description": "Sistema web para gestión de fotografía escolar con QR codes y pagos",
  "author": "LookEscolar",
  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "dev:db": "supabase start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:auth": "vitest --run auth.test.ts auth-api.test.ts LoginForm.test.tsx",
    "test:auth:watch": "vitest --watch auth.test.ts auth-api.test.ts LoginForm.test.tsx",
    "test:mvp": "tsx scripts/run-mvp-tests.ts",
    "test:mvp:quick": "npm run test __tests__/e2e/complete-mvp-workflow.test.ts && npm run test __tests__/api-critical-endpoints.test.ts",
    "test:components": "vitest __tests__/components/",
    "test:apis": "vitest __tests__/api-critical-endpoints.test.ts",
    "guard:background": "tsx scripts/background-guard.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed.ts",
    "db:types": "supabase gen types typescript --local > types/database.ts",
    "db:types:update": "tsx scripts/update-types.ts",
    "db:status": "supabase status",
    "db:diff": "supabase db diff",
    "qr:class": "tsx scripts/generate-qr-class.ts",
    "storage:cleanup": "tsx scripts/storage-cleanup.ts",
    "export:orders": "tsx scripts/export-orders.ts",
    "rotate:tokens": "tsx scripts/rotate-tokens.ts",
    "metrics:egress": "tsx scripts/metrics-egress.ts",
    "logs:tail": "tsx scripts/logs-tail.ts",
    "dev:db:logs": "supabase logs",
    "storage:inspect": "tsx scripts/storage-inspect.ts",
    "queue:status": "tsx scripts/queue-status.ts",
    "perf:report": "tsx scripts/perf-report.ts",
    "metrics:ratelimit": "tsx scripts/metrics-ratelimit.ts",
    "seed:v1": "tsx scripts/seed-v1.ts",
    "security:check": "tsx scripts/security-check.ts",
    "test:security": "vitest __tests__/security/",
    "test:integration": "vitest __tests__/integration/",
    "test:e2e": "vitest __tests__/e2e/",
    "performance:test": "tsx scripts/performance-test.ts",
    "test:backend": "tsx scripts/test-backend-mvp.ts",
    "test:usability": "tsx scripts/run-usability-tests.ts",
    "test:usability:a11y": "npx playwright test __tests__/usability/accessibility-comprehensive.test.ts",
    "test:usability:responsive": "npx playwright test __tests__/usability/responsive-design.test.ts",
    "test:usability:workflows": "npx playwright test __tests__/usability/user-journey-workflows.test.ts",
    "test:usability:performance": "npx playwright test __tests__/usability/performance-web-vitals.test.ts",
    "test:usability:browsers": "npx playwright test __tests__/usability/cross-browser-compatibility.test.ts",
    "test:usability:visual": "npx playwright test __tests__/usability/visual-regression.test.ts",
    "test:usability:errors": "npx playwright test __tests__/usability/error-handling-edge-cases.test.ts",
    "test:usability:report": "tsx scripts/run-usability-tests.ts && open test-reports/usability/usability-report.html",
    "test:comprehensive": "tsx scripts/run-comprehensive-tests.ts",
    "test:comprehensive:unit": "tsx scripts/run-comprehensive-tests.ts --stage=unit",
    "test:comprehensive:integration": "tsx scripts/run-comprehensive-tests.ts --stage=integration",
    "test:comprehensive:e2e": "tsx scripts/run-comprehensive-tests.ts --stage=e2e",
    "test:comprehensive:ci": "tsx scripts/run-comprehensive-tests.ts --env=ci --bail --no-coverage",
    "test:tdd": "vitest __tests__/tdd-critical-endpoints.test.ts --reporter=verbose",
    "test:security:enhanced": "vitest __tests__/security-enhanced.test.ts --reporter=verbose",
    "test:performance:comprehensive": "vitest __tests__/performance-comprehensive.test.ts --reporter=verbose --testTimeout=45000",
    "test:integration:workflows": "vitest __tests__/integration-workflows.test.ts --reporter=verbose --testTimeout=60000",
    "test:perf": "vitest run tests/perf/smoke.test.ts",
    "validate:production": "tsx scripts/validate-production-readiness.ts",
    "security:audit": "tsx scripts/security-audit.ts",
    "db:setup": "node scripts/setup-db.js",
    "deploy": "scripts/deploy.sh",
    "deploy:docker": "scripts/deploy.sh --type docker",
    "deploy:vercel": "scripts/deploy.sh --type vercel",
    "deploy:manual": "scripts/deploy.sh --type manual",
    "deploy:staging": "scripts/deploy.sh --env staging",
    "docker:build": "docker build -t lookescolar:latest .",
    "docker:run": "docker-compose -f docker-compose.production.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "bundle:analyze": "node scripts/analyze-bundle.js analyze",
    "bundle:deps": "node scripts/analyze-bundle.js deps", 
    "bundle:recommendations": "node scripts/analyze-bundle.js recommendations",
    "perf:dashboard": "tsx scripts/performance-dashboard.ts"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.84.1",
    "@upstash/ratelimit": "^2.0.0",
    "@upstash/redis": "^1.29.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "express-rate-limit": "^7.4.1",
    "jose": "^5.9.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.536.0",
    "mercadopago": "^2.8.0",
    "nanoid": "^5.1.5",
    "next": "^15.4.5",
    "p-limit": "^6.2.0",
    "pdfkit": "^0.15.1",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.2",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.10",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.2.0",
    "@types/pdfkit": "^0.13.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.2",
    "axe-playwright": "^2.0.3",
    "chalk": "^5.5.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "form-data": "^4.0.4",
    "jsdom": "^25.0.1",
    "lighthouse": "^12.3.2",
    "node-fetch": "^2.7.0",
    "playwright": "^1.48.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "supabase": "^1.213.4",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
